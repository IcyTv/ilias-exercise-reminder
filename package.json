	},
	"devDependencies": {
		"@types/body-parser": "^1",
		"@types/concurrently": "^6",
		"@types/express": "^4",
		"@types/lowdb": "^1",
		"@types/tough-cookie": "^4",
		"@types/uuid": "^8",
		"concurrently": "^6.0.2",
		"nodemon": "^2.0.7",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@typeit/discord": "^4.0.9",
		"body-parser": "^1.19.0",
		"cheerio": "^1.0.0-rc.6",
		"discord.js": "^12.5.3",
		"dotenv": "^9.0.0",
		"express": "^4.17.1",
		"got": "^11.8.2",
		"lowdb": "^1.0.0",
		"node-json-db": "^1.3.0",
		"pug": "^3.0.2",
		"tough-cookie": "^4.0.0",
		"tslib": "^2.2.0",
		"uuid": "^8.3.2"
	}
