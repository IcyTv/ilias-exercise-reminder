{
	"name": "ilias-exercise-reminder",
	"version": "1.0.0",
	"bin": "dist/cli.js",
	"license": "MIT",
	"unused": {
		"build:executable": "caxa -d dist -c \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/cli.js\" -o ./app.exe"
	},
	"scripts": {
		"start": "ts-node --files --pretty src/index.ts",
		"type-check": "tsc --noEmit",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "webpack",
		"build:src": "yarn build:types && yarn build:js",
		"build:executable": "pkg -C Brotli .",
		"build": "yarn clean && yarn build:src && yarn build:executable",
		"clean": "rimraf dist",
		"clean:cached": "rimraf .yarn/cache"
	},
	"devDependencies": {
		"@babel/cli": "^7.13.16",
		"@babel/core": "^7.14.0",
		"@babel/preset-typescript": "^7.13.0",
		"@types/babel__core": "^7",
		"@types/babel__preset-env": "^7",
		"@types/body-parser": "^1",
		"@types/concurrently": "^6",
		"@types/dotenv-webpack": "^7",
		"@types/express": "^4",
		"@types/inquirer": "^7.3.1",
		"@types/lowdb": "^1",
		"@types/prompt-sync": "^4",
		"@types/rimraf": "^3",
		"@types/tough-cookie": "^4",
		"@types/underscore": "^1",
		"@types/uuid": "^8",
		"babel-loader": "^8.2.2",
		"caxa": "^1.0.0",
		"concurrently": "^6.0.2",
		"dotenv-webpack": "^7.0.2",
		"node-loader": "^2.0.0",
		"nodemon": "^2.0.7",
		"progress-bar-webpack-plugin": "^2.1.0",
		"rimraf": "^3.0.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4",
		"webpack": "^5.36.2",
		"webpack-cli": "latest",
		"webpack-node-externals": "^3.0.0"
	},
	"dependencies": {
		"@babel/preset-env": "^7.14.1",
		"@googleapis/calendar": "^0.2.0",
		"@typeit/discord": "^4.0.9",
		"body-parser": "^1.19.0",
		"boxednode": "^1.10.1",
		"chalk": "^4.1.1",
		"cheerio": "^1.0.0-rc.6",
		"commander": "^7.2.0",
		"dayjs": "^1.10.4",
		"discord.js": "^12.5.3",
		"dotenv": "^9.0.0",
		"express": "^4.17.1",
		"googleapis": "^73.0.0",
		"got": "^11.8.2",
		"inquirer": "^8.0.0",
		"keytar": "^7.7.0",
		"lowdb": "^1.0.0",
		"moment": "^2.29.1",
		"node-json-db": "^1.3.0",
		"open": "^8.0.7",
		"pkg": "^5.1.0",
		"prompt-sync": "^4.2.0",
		"pug": "^3.0.2",
		"supports-color": "^9.0.0",
		"tough-cookie": "^4.0.0",
		"tslib": "^2.2.0",
		"underscore": "^1.13.1",
		"uuid": "^8.3.2"
	},
	"pkg": {
		"script": "dist/cli.js",
		"assets": "dist/native.node",
		"outputPath": "dist",
		"compress": "Brotli"
	}
}
